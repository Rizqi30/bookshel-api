{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "no-shadow": "off",
        "implicit-arrow-linebreak": "off",
        "eqeqeq": "off",
        "consistent-return": "off"
    }
}
